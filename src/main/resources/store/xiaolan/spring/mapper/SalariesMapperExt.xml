<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="store.xiaolan.spring.mapper.SalariesMapperExt">
    <resultMap id="salary" type="store.xiaolan.spring.domian.Salaries">
        <result column="emp_no" property="empNo"/>
        <result column="salary" property="salary"/>
        <result column="from_date" property="fromDate"/>
        <result column="to_date" property="toDate"/>
    </resultMap>
    <select id="listSalaries" resultMap="salary">
        select emp_no,salary,from_date,to_date from salaries limit 20
    </select>

</mapper>